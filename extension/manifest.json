{
	"manifest_version": 3,
	"name": "VectorPDF Extension",
	"version": "1.0",
	"description": "Una extensi√≥n de Chrome que se comunica con VectorPDF",
	"icons": {
	  "16": "icon16.png",
	  "48": "icon48.png",
	  "128": "icon128.png"
	},
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	  }
	},
	"permissions": ["storage", "activeTab", "scripting", "downloads"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
  }
  